<script>
    const audio = new Audio();

    function playAudio(soundName) {
        if (!audio.paused){
            audio.pause();
        }
        audio.src = '/audio/categories/' + soundName;
        audio.play();
    }

    function zoomInwithAudio(areaName, soundName) {
        if (!audio.paused){
            audio.pause();
        }
        audio.src = '/audio/categories/26/' + soundName;

        audio.addEventListener("ended", function(){
            location.href= '/street/' + areaName;
        });

        audio.play();
    }
    function selectCategory(categoryName) {
        if (!audio.paused){
            audio.pause();
        }
        audio.src = '/audio/categories/' + categoryName + '.mp3';

        audio.addEventListener("ended", function(){
            location.href= '/category/' + categoryName;
        });

        audio.play();
    }
</script>